<% if @return_info == "Mate" %>
$(".modal-body").replaceWith("<%= j render "baskets/mate" %>");
$('#myModal').modal('show');
$(".save_btn").show();
<% elsif @return_info == "TRUE" %>
alert("放入成功");
$("#basket_cage_list").replaceWith("<%= j render "frameworks/framework_baskets" %>")
$(".basket_show").replaceWith("<%= j render "baskets/show" %>");
$(".basket_show").show();
<% elsif @return_info == "FALSE" %>
  alert("对不起，该小鼠无法放入其中");
$(".basket_show").replaceWith("<%= j render "baskets/show" %>");
$(".basket_show").show();
<% elsif @return_info == "Move_Out" %>
$("#basket_cage_list").replaceWith("<%= j render "frameworks/framework_baskets" %>")
$(".basket_show").replaceWith("<%= j render "baskets/show" %>");
$(".basket_show").show();
<% end %>
$(".tddiv").removeClass("active");
$("#cage_<%= @basket.id %>").addClass("active");
