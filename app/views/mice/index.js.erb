<% if params[:strain_id] %>
$(".mice_strain").html("小鼠信息[<%= Strain.find(params[:strain_id]).name %>]");
$("#mice").DataTable().ajax.url("<%= get_data_mice_path(strain_id: params[:strain_id]) %>");
$("#mice").DataTable().ajax.reload();
<% else %>
$(".mice_strain").html("小鼠信息");
$("#mice").DataTable().ajax.url("<%= get_data_mice_path %>");
$("#mice").DataTable().ajax.reload();
<% end %>
